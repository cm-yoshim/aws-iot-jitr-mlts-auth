{
  "compilerOptions": {
    "target":"ES2018",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2016",
      "es2017.object",
      "es2017.string",
      "es2020",
      "es2020.promise"
    ],
    "esModuleInterop": true,
    "declaration": false,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization":false,
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "baseUrl": ".",
  "exclude": [
    "cdk.out"
  ]
}
